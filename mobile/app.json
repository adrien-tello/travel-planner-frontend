{
  "expo": {
    "name": "Wanderlust",
    "slug": "wanderlust",
    "version": "1.0.0",
    "icon":"./assets/image/logo.png",
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Wanderlust to use your location to show nearby attractions and optimize your trips."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/wayfinder-splash-logo.png",
          "color": "#3B82F6",
          "sounds": []
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox"
        }
      ],
      "expo-secure-store",
      "@react-native-community/datetimepicker",
      "expo-font"
    ],
    "scheme": "wanderlust",
    "platforms": ["ios", "android", "web"],
    "ios": {
      "bundleIdentifier": "com.wanderlust.app",
      "config": {
        "googleMapsApiKey": "GOOGLE_MAPS_API_KEY"
      }
    },
    "android": {
      "package": "com.wanderlust.app",
      "config": {
        "googleMaps": {
          "apiKey": "GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "extra": {
      "BACKEND_ENDPOINT": "https://81034f13d712.ngrok-free.app/api"
    }
  }
}
