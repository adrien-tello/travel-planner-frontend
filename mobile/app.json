{
  "expo": {
    "name": "Wanderlust",
    "slug": "wanderlust",
    "version": "1.0.0",
    "icon": "./assets/image/logo.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Wanderlust to use your location to show nearby attractions and optimize your trips."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/image/icon.png",
          "color": "#3B82F6",
          "sounds": []
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox"
        }
      ],
      "expo-secure-store",
      "@react-native-community/datetimepicker",
      "expo-font"
    ],
    "scheme": "wanderlust",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "bundleIdentifier": "com.wanderlust.app",
      "config": {
        "googleMapsApiKey": "GOOGLE_MAPS_API_KEY"
      }
    },
    "android": {
      "package": "com.wanderlust.app",
      "config": {
        "googleMaps": {
          "apiKey": "GOOGLE_MAPS_API_KEY"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "extra": {
      "BACKEND_ENDPOINT": "https://ccf76cd50c20.ngrok-free.app/api",
      "router": {},
      "eas": {
        "projectId": "c90376c3-f93c-411a-b149-f2643deb6d11"
      }
    }
  }
}
